

.mission-info_title {
  margin-bottom: 10px;
  font-size: medium;
}

.mission-info_desc {
  font-size: small;
  color: darkgray;
}

.mission-info_credit {
  font-size: small;
  color: darkgray;
  margin-left: 10px;
}
.swiper-image{
  display: block;
  height: 100%; 
  width: 100%;
  /* background-color: darkgray; */
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}
.jumpSetModel { 
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0,0,0,0.4);
  z-index: 9999;
 }
 .jumpSetBox {
  border-radius: 36rpx;
  width: 80%;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
 }
 .m-title{
  font-size: 36rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #030303;
}
.m-content{
  flex: 1;
  width: 80%;
  overflow: hidden;
  padding: 35rpx 0;
}
.m-button{
  font-size: 34rpx;
  width: 100%;
   height: 80rpx;
  line-height: 80rpx;
  display: flex;
  flex-direction: row;
  border-top: 1rpx solid #dedede;
}
.m-cancel, .m-confirm{
  width: 50%;
  text-align: center;
}
.m-cancel{
  color: #ccc;
  border-right: 1rpx solid #dedede;
}